{
  "name": "crowdfund-backend-express",
  "version": "1.0.0",
  "description": "Crowdfunding backend with Express, Sequelize, PostgreSQL, Redis",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:dummy": "ts-node src/scripts/seedDatabase.ts",
    "seed:admin": "ts-node src/scripts/createAdminUsers.ts",
    "db:clear": "ts-node src/scripts/clearDatabase.ts",
    "db:reset": "npm run db:clear && npm run seed:dummy",
    "db:setup": "npm run migrate && npm run seed:admin",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.21.0",
    "pino-http": "^8.6.1",
    "redis": "^4.6.12",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.9",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "express",
    "sequelize",
    "postgresql",
    "redis",
    "jwt",
    "docker"
  ],
  "author": "",
  "license": "ISC"
}
